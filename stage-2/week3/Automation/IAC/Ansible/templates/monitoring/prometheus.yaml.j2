global:
  scrape_interval: 20s

scrape_configs:
  - job_name: 'prometheus'
    scrape_interval: 20s
    static_configs:
    - targets: ['localhost:9090']

  - job_name: 'node'
    static_configs:
    - targets: ['node-exporter:9100']
    
  - job_name: 'server_biznet'
    scrape_interval: 20s
    static_configs:
    - targets: ['103.127.136.82:9100','103.196.153.93:9100']

  - job_name: 'server_gcp'
    scrape_interval: 20s
    static_configs:
    - targets: ['34.124.180.2:9100']